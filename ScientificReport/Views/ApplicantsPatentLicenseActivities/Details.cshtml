@using Microsoft.AspNetCore.Mvc.Localization
@model ScientificReport.DAL.Entities.ApplicantsPatentLicenseActivities
@inject IViewLocalizer Localizer

@{
    ViewData["Title"] = Localizer["Details"];
}

<h3>@Localizer["Details"]</h3>

<div>
    <h5>@Localizer["Applicants Patent License Activities"]</h5>
    <hr />
    <dl class="row">
        <dt class = "col s2">
            @Localizer[Html.DisplayNameFor(model => model.ApplicantId)]
        </dt>
        <dd class = "col s10">
            @Html.DisplayFor(model => model.ApplicantId)
        </dd>
        <dt class = "col s2">
            @Localizer[Html.DisplayNameFor(model => model.PatentLicenseActivityId)]
        </dt>
        <dd class = "col s10">
            @Html.DisplayFor(model => model.PatentLicenseActivityId)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id" class="waves-effect btn-small waves-light">@Localizer["Edit"]</a>
    <a asp-action="Index" class="btn-small blue darken-3">@Localizer["Back to List"]</a>
</div>
