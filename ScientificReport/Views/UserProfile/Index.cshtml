@model IEnumerable<ScientificReport.Models.UserProfile>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Type)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MiddleName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BirthYear)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GraduationYear)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ScientificDegree)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.YearDegreeGained)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AcademicStatus)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.YearDegreeAssigned)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Position)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsApproved)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MiddleName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <th>
                @Html.DisplayFor(modelItem => item.BirthYear)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.GraduationYear)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.ScientificDegree)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.YearDegreeGained)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.AcademicStatus)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.YearDegreeAssigned)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.Position)
            </th>
            <th>
                @Html.DisplayFor(modelItem => item.IsApproved)
            </th>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
