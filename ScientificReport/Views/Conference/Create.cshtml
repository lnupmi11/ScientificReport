@model ScientificReport.DAL.Entities.Conference

@{
    ViewData["Title"] = "Create";
}

<h1>Create</h1>

<h4>Conference</h4>
<hr />
<div class="row">
    <div class="col s12">
        <form asp-action="Create" class="col s12">
            <div asp-validation-summary="ModelOnly" class="validate"></div>
            <div class="row">
                <div class="input-field col s6">
                    <input asp-for="Topic" type="text" class="validate">
                    <label asp-for="Topic">Topic</label>
                    <span class="helper-text" asp-validation-for="Topic" data-error="some data is in bad format" data-success=""></span>
                </div>
            </div>
            <div class="row">
                <div class="input-field col s6">
                    <input asp-for="Date" type="date" class="validate">
                    <label asp-for="Date">Password</label>
                    <span asp-validation-for="Date" data-error="" data-success=""></span>
                </div>
            </div>
            <div class="row">
                <input type="submit" value="Create" class="waves-effect btn-small  amber accent-2"/>
                <a asp-action="Index" class="btn-small blue darken-3">Cancel</a>
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
