@model ScientificReport.DAL.Entities.Grant

@{
    ViewData["Title"] = "Edit";
}

<h1>Edit</h1>

<h4>Grant</h4>
<hr />
<div class="row">
    <div class="col s12">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="validate"></div>
            <input type="hidden" asp-for="Id" />
            <div class="row">
                <input type="submit" value="Save" class="waves-effect btn-small amber accent-2"/>
                <a asp-action="Index" class="btn-small blue darken-3">Cancel</a>
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
